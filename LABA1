#include <iostream>
#include <string.h>

using namespace std;
int main() 
{ 
string number;
cout << "Vvedit` dani: ";
cin >> number;
    if(number.size()==5) 
         { if( (number[0] == number[4]) && (number[1] == number[3]) )
		         cout << "Palindrom!" << endl;
    	   else 
        	   cout << "Ne palindrom!" << endl;  }
    else
      cout << "Vvedit` 5 tsyfr!" << endl;
return 0; 
}
